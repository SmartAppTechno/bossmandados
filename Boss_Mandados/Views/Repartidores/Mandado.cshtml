@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Detalles del Mandado</h2>
    </div>
</div>

<div class="wrapper-content">
    <div class="ibox-content">
        <div class="table-responsive">
            <table class="table table-striped table-bordered table-hover tabla-buscador">
                <thead>
                    <tr>
                        <th>Servicio</th>
                        <th>Comentarios</th>
                        <th>Ubicación</th>
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in ViewBag.mandado)
                    {
                        <tr>
                            <td>
                                @item.servicio
                            </td>
                            <td>
                                @item.comentarios
                            </td>
                            <td>
                                <p class="btn btn-w-m btn-primary"><a href="http://maps.google.com/?q=@item.latitud,@item.longitud"><i class="fa fa-map-marker"></i></a></p>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
<p class="btn btn-w-m btn-primary"><a href="@Url.Action("Mandados", "Repartidores", new { id = ViewBag.mandado_id })">Volver</a></p>