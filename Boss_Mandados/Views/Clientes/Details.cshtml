@model Boss_Mandados.Models.manboss_clientes
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Detalles del Cliente</h2>
    </div>
</div>
<div class="wrapper-content">
    <div class="ibox-content">
        <form role="form" action="" method="post" class="form-horizontal">
            <div class="form-group">
                <label class="col-sm-2 control-label">Nombre</label>
                <label class="col-sm-10 control-label">@Model.nombre</label>
            </div>
            <div class="hr-line-dashed"></div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Correo</label>
                <label class="col-sm-10 control-label">@Model.correo</label>
            </div>
            <div class="hr-line-dashed"></div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Red Social</label>
                <label class="col-sm-10 control-label"><i class="fa fa-@Model.red_social"></i></label>
            </div>
            <div class="hr-line-dashed"></div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Dirección</label>
                <label class="col-sm-10 control-label">@Model.direccion</label>
            </div>
            <div class="hr-line-dashed"></div>
            <div class="form-group">
                <label class="col-sm-2 control-label">Teléfono</label>
                <label class="col-sm-10 control-label">@Model.telefono</label>
            </div>
        </form>
    </div>
</div>
<p class="btn btn-w-m btn-primary">@Html.ActionLink("Volver", "Index")</p>